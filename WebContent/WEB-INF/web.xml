<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="3.0"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

	<display-name>Retail app</display-name>
	<welcome-file-list>
		<welcome-file>housing.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>Dilemma Servlet</servlet-name>
		<servlet-class>com.ibm.watson.ta.retail.DemoServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>Dilemma Servlet</servlet-name>
		<url-pattern>/demo</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Housing Servlet</servlet-name>
		<servlet-class>com.ibm.watson.ta.retail.ImportHousingServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>Housing Servlet</servlet-name>
		<url-pattern>/housing</url-pattern>
	</servlet-mapping>

	<mime-mapping> 
	    <extension>svg</extension>
	    <mime-type>image/svg+xml</mime-type> 
	</mime-mapping>
</web-app>